{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ["./src", "./README.md"],
        "includePattern": "\\.js$"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash/",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "collapse": true,
        "sort": true,
        "meta": {
            "title": "IJO Docs"
        }
    }
}